{
  "name": "plotly-graph-1.0",
  "version": "1.0",
  "description": "Example mod with multiple widgets",
  "cdn": ["https://cdn.plot.ly/plotly-latest.min.js"],
  "widgets": [
    {
      "name": "plotly-line-graph",
      "type": "widget",
      "description": "A real-time line graph using Plotly",
      "component": "LineGraph.jsx",
      "options": {
        "defaultHeight": 300,
        "defaultWidth": 400
      }
    },
    {
      "name": "plotly-scatter-plot",
      "type": "widget", 
      "description": "A scatter plot widget using Plotly",
      "component": "ScatterPlot.jsx",
      "options": {
        "defaultHeight": 400,
        "defaultWidth": 400
      }
    }
  ]
}
  